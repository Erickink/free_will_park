<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.FreeWillPark" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_500</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/purple_500</item>
        <item name="colorSecondaryVariant">@color/purple_500</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">@android:color/transparent</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.FreeWillPark.NoActionBar">
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.FreeWillPark.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.FreeWillPark.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="CustomTheme" parent="Theme.AppCompat.Light.NoActionBar"/>

    <style name="CustomActionBar" parent="@style/ThemeOverlay.AppCompat.Dark.ActionBar">
        <item name="android:windowActionBarOverlay">true</item>
        <!-- Support library compatibility -->
        <item name="windowActionBarOverlay">true</item>
    </style>

    <style name="Theme.FreeWillPark.FullScreen" parent="@style/Theme.AppCompat.Light.NoActionBar">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>

    <style name="Theme.FreeWillPark.Memory">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:windowLightStatusBar">true</item>
        <item name="android:windowLightNavigationBar">true</item>
        <item name="android:statusBarColor">@android:color/transparent</item>
        <item name="android:windowDrawsSystemBarBackgrounds">true</item>
        <item name="android:windowTranslucentStatus">true</item>
        <item name="android:windowTranslucentNavigation">true</item>
        <item name="android:navigationBarColor">@android:color/transparent</item>
    </style>

    <style name="fab2">
        <item name="colorOnSecondary">#FF0048FF</item>
        <item name="colorSecondary">#FFFFFFFF</item>

        <item name="colorPrimary">#FF0000FF</item>
        <item name="colorPrimaryVariant">#FF0000FF</item>
        <item name="colorOnPrimary">#FF0000FF</item>
        <!-- Secondary brand color. -->

        <item name="colorSecondaryVariant">#FF0000FF</item>
    </style>

    <style name="fab3">
        <item name="colorOnSecondary">#1FBF25</item>
        <item name="colorSecondary">#FFFFFFFF</item>
    </style>

    <style name="fab4">
        <item name="colorOnSecondary">#FFE500</item>
        <item name="colorSecondary">#FFFFFFFF</item>
    </style>

    <style name="fabDelete">
        <item name="colorOnSecondary">#FFFFFFFF</item>
        <item name="colorSecondary">#FF3939</item>
    </style>

    <style name="fabV">
        <item name="colorOnSecondary">#FFFFFFFF</item>
        <item name="colorSecondary">#FFFFFFFF</item>
    </style>

    <style name="fabadd">
        <item name="colorOnSecondary">#FFFFFFFF</item>
        <item name="colorSecondary">#2BD598</item>
    </style>
</resources>